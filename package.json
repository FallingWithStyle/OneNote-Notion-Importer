{
  "name": "oni",
  "version": "1.0.0",
  "description": "OneNote to Notion Importer (ONI) - A powerful, offline-first CLI tool for migrating OneNote content to Notion while preserving your notebook structure and giving you complete control over what gets imported.",
  "main": "dist/index.js",
  "bin": {
    "oni": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "onenote",
    "notion",
    "migration",
    "cli",
    "converter",
    "import",
    "export"
  ],
  "author": "Patrick",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.4.0",
    "@types/winston": "^2.4.4",
    "commander": "^14.0.1",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "winston": "^3.17.0"
  }
}
